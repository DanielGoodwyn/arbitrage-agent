# ============================================
# Autonomous Global Event Arbitrage Agent
# Environment Configuration
# ============================================
# Copy this file to .env and fill in your keys

# --- Robinhood ---
ROBINHOOD_USERNAME=your_robinhood_email
ROBINHOOD_PASSWORD=your_robinhood_password
ROBINHOOD_MFA_CODE=
ROBINHOOD_DEVICE_TOKEN=

# --- Senso (Context OS) ---
SENSO_API_KEY=your_senso_api_key
SENSO_BASE_URL=https://api.senso.ai

# --- Airbyte ---
AIRBYTE_API_KEY=your_airbyte_api_key
AIRBYTE_BASE_URL=https://api.airbyte.com
AIRBYTE_WORKSPACE_ID=your_workspace_id

# --- Tavily (Web Search) ---
TAVILY_API_KEY=your_tavily_api_key

# --- Reka (Vision API) ---
REKA_API_KEY=your_reka_api_key
REKA_BASE_URL=https://api.reka.ai

# --- Neo4j (Knowledge Graph) ---
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password

# --- Fastino Labs (Pioneer) ---
FASTINO_API_KEY=your_fastino_api_key
FASTINO_BASE_URL=https://api.pioneer.ai

# --- Yutori (N1 Navigator) ---
YUTORI_API_KEY=your_yutori_api_key
YUTORI_BASE_URL=https://platform.yutori.com

# --- Numeric (Accounting) ---
NUMERIC_API_KEY=your_numeric_api_key
NUMERIC_BASE_URL=https://api.numeric.io

# --- Modulate (Voice API) ---
MODULATE_API_KEY=your_modulate_api_key
MODULATE_BASE_URL=https://modulate-developer-apis.com

# --- IONOS Hosting ---
IONOS_SFTP_HOST=DanielGoodwyn.com
IONOS_SFTP_USER=your_sftp_username
IONOS_SFTP_PASSWORD=your_sftp_password
IONOS_SFTP_PORT=22
IONOS_DEPLOY_PATH=/arbitrage

# --- Deployment ---
RENDER_API_KEY=your_render_api_key
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-west-2

# --- Agent Config ---
AGENT_CYCLE_INTERVAL_SECONDS=30
ARBITRAGE_SCORE_THRESHOLD=0.75
ANOMALY_ALERT_THRESHOLD=0.95
LOG_LEVEL=INFO
